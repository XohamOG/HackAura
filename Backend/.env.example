# Backend Environment Variables - Lisk Sepolia Configuration

# Server Configuration
PORT=4000

# GitHub OAuth Configuration
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_BEARER_TOKEN=your_github_personal_access_token

# Google Gemini AI API
GEMINI_API_KEY=your_gemini_api_key

# Lisk Sepolia Blockchain Configuration
NETWORK=lisk
NETWORK_NAME=Lisk Sepolia Testnet
NETWORK_RPC_URL=https://rpc.sepolia-api.lisk.com
NETWORK_CHAIN_ID=4202
NETWORK_SYMBOL=ETH
NETWORK_EXPLORER=https://sepolia-blockscout.lisk.com

# Smart Contract Addresses on Lisk Sepolia
BOUNTY_ESCROW_ADDRESS=0xCc47A9e11De3aA189cD801fD54054493C5e11a68
REPO_REGISTRY_ADDRESS=0xa50C8481E6a39681b7B881e9884e8a03567BAdFF

# Private Key for Contract Interactions (DO NOT COMMIT THIS!)
PRIVATE_KEY=your_wallet_private_key_here

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# JWT Secret (for authentication)
JWT_SECRET=your_jwt_secret_here
